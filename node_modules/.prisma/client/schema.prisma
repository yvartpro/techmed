datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Equipment {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
}

model Activity {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  date        DateTime @default(now())
}
